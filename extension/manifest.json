{
  "manifest_version": 3,
  "name": "Дополнение модератора",
  "description": "Расширение добавляет новые специальные возможности в Народную карту для модераторов",
  "version": "1.1.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://n.maps.yandex.ru/*"],
      "css": [
        "/resources/style.css" 
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "debounce.js",
        "/resources/init.js",
        "/resources/notifications.js",
        "/resources/module/address.js",
        "/resources/module/get.user.js",
        "/resources/module/get.profile.js"
      ]
    }
  ],
  "permissions": ["storage"]
}